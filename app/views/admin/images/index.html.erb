<%= render partial: "shared/hero", locals: { title: "Admin", subtitle: "Images" } %>

<div class="container">
  <div class="">
    <%= link_to "Add Image", new_admin_image_path %>
    <table class="table table-hover">
      <thead>
        <tr>
          <th scope="col">#<th>
          <th scope="col">Title<th>
          <th scope="col">Description<th>
          <th scope="col">Price<th>
        </tr>
      </thead>
      <tbody>
        <%= render partial: "admin_image", collection: @images %>
      </tbody>
    </table>
  </div>
</div>